{
  "name": "bloghub",
  "version": "1.0.0",
  "description": "A full-stack web application for creating, sharing, and interacting with blog posts",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm run dev",
    "start:client": "cd client && npm run dev",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "install:all": "concurrently \"cd server && npm install\" \"cd client && npm install\"",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "keywords": [
    "blog",
    "react",
    "node",
    "express",
    "mongodb",
    "fullstack"
  ],
  "author": "BlogHub Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/BlogHub.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/BlogHub/issues"
  },
  "homepage": "https://github.com/your-username/BlogHub#readme"
}
